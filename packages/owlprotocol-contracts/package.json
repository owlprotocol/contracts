{
    "name": "@owlprotocol/contracts",
    "version": "0.1.0",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/oyunprotocol/nft-launcher-contracts.git"
    },
    "homepage": "https://github.com/oyunprotocol/nft-launcher-contracts.git",
    "copyright": "Copyright 2021 OYUNPROTOCOL",
    "license": "MIT",
    "scripts": {
        "hardhat:compile": "npx hardhat compile",
        "hardhat:node": "npx hardhat node",
        "clean:types": "rimraf types && npx hardhat clean",
        "coverage": "npx hardhat coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
        "coverage:gsn": "npx hardhat --network local coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
        "test": "npx hardhat test",
        "compile": "npx hardhat clean && npx hardhat compile",
        "test:parallel": "npx hardhat test --parallel",
        "build": "npm run compile",
        "docs": "DOCS=../owlprotocol-contracts-docs/docs/contract-docs && rm -f $DOCS/* && hardhat docgen && echo '# Solidity Reference' > $DOCS/index.md",
        "generate-types": "npm-run-all -p generate-types:web3 generate-types:ethers",
        "generate-types:web3": "typechain --target web3-v1 --out-dir=types/web3 'artifacts/contracts/**/+([a-zA-Z0-9_]).json'",
        "generate-types:ethers": "typechain --target=ethers-v5 --out-dir=types/ethers 'artifacts/contracts/**/+([a-zA-Z0-9_]).json'"
    },
    "peerDependencies": {
        "@ethersproject/abi": "^5.4.0",
