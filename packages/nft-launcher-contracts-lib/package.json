{
    "name": "@oyunprotocol/nft-launcher-contracts",
    "version": "0.1.0",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/oyunprotocol/nft-launcher-contracts.git"
    },
    "homepage": "https://github.com/oyunprotocol/nft-crafter.git",
    "copyright": "Copyright 2021 OYUNPROTOCOL",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf lib",
        "test": "cross-env NODE_ENV=test mocha 'lib/**/*.test.js'",
        "start": "node lib/index.js",
        "startServer": "node lib/server",
        "build": "tsc",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "copy": "npm-run-all copy:prepublish",
        "copy:prepublish": "mkdir -p lib && cp package.json lib/ && cp README.md lib/ && cp ../../LICENSE lib/",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:staged": "lint-staged",
        "postinstall": "cd ../../ && husky install && npm run build",
        "prepublishOnly": "npm run test"
    },
    "dependencies": {
        "ajv": "^8.10.0",
        "axios": "^0.27.2",
        "bn.js": "^5.2.1",
        "colormap": "^2.3.2",
        "dotenv": "^10.0.0",
        "express": "^4.18.1",
        "http-errors": "^2.0.0",
        "ipfs-http-client": "^56.0.1",
        "merge-images": "^2.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/big-integer": "^0.0.31",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.4",
        "@types/colormap": "^2.3.1",
        "@types/http-errors": "^1.8.2",
        "@types/merge-images": "^1.2.1",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.19",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "bignumber.js": "^9.0.2",
        "canvas": "^2.9.0",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^3.4.1",
        "ipfs-core-types": "^0.10.1",
        "lint-staged": "^11.2.6",
        "mocha": "^9.1.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.4.0",
        "web3-utils": "^1.6.1"
    },
    "files": [
        "*",
        "!test",
        "!./**/*.test.*"
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "publishConfig": {
        "directory": "lib"
    }
}
