{
  "name": "@owlprotocol/nft-launcher-contracts",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo containing NFT Launcher Contract packages",
  "license": "Apache License 2.0",
  "contributors": ["Leo Vigna", "Hrik B.", "Corban Villa"],
  "scripts": {
    "build": "npm-run-all -p build:sol build:lib",
    "build:sol": "cd packages/nft-launcher-contracts-sol && npm run build",
    "build:lib": "cd packages/nft-launcher-contracts-lib && npm run build",
    "test": "npm-run-all -p test:sol test:lib",
    "test:sol": "cd packages/nft-launcher-contracts-sol && npm run test",
    "test:lib": "cd packages/nft-launcher-contracts-lib && npm run test",
    "clean": "npm-run-all -p clean:lib clean:node_modules",
    "clean:lib": "rimraf packages/*/lib",
    "clean:node_modules": "rimraf packages/*/node_modules",
    "postinstall": "husky install && npm run build",
    "lint:staged": "echo 'No lint script'"
  },
  "dependencies": {
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5"
  }
}
