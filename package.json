{
    "name": "@oyunprotocol/nft-crafter",
    "version": "0.1.0",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/oyunprotocol/nft-crafter.git"
    },
    "homepage": "https://github.com/oyunprotocol/nft-crafter.git",
    "copyright": "Copyright 2021 OYUNPROTOCOL",
    "license": "MIT",
    "scripts": {
        "hardhat:compile": "npx hardhat compile",
        "hardhat:deploy": "npx hardhat run --network localhost scripts/deploy.ts",
        "hardhat:node": "npx hardhat node",
        "clean": "npm-run-all -p clean:scripts clean:test clean:types",
        "clean:scripts": "rimraf scripts",
        "clean:test": "rimraf test",
        "clean:types": "rimraf typechain",
        "test": "npx hardhat test",
        "start": "npx hardhat run --network localhost scripts/deploy.t",
        "test:parallel": "npx hardhat test --parallel",
        "compile": "npx hardhat clean && npx hardhat compile && npx hardhat run src/generate.ts",
        "deploy": "npx hardhat src/deploy.ts",
        "deploy:test": "cross-env NODE_ENV=test ts-node src/deploy.ts",
        "lint": "eslint src test",
        "lint:fix": "eslint src test --fix",
        "lint:staged": "lint-staged",
        "docs:solidity": "solidity-docgen --solc-module solc-0.8 -i contracts -o docs/solidity",
        "docs:ts": "typedoc",
        "prepare": "husky install && npm run hardhat:compile",
        "prepublishOnly": "npm run test"
    },
    "dependencies": {
        "ajv": "^8.10.0",
        "dotenv": "^10.0.0",
        "ipfs-http-client": "^56.0.1",
        "merge-images": "^2.0.0"
